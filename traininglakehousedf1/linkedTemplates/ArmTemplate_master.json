{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "traininglakehousedf1"
		},
		"Curation Spark Cluster_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'Curation Spark Cluster'"
		},
		"Synapse Serverless_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Synapse Serverless'"
		},
		"salesdbsource_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'salesdbsource'"
		},
		"traininglakehouse_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'traininglakehouse'"
		},
		"websitetrackinglogs_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'websitetrackinglogs'"
		},
		"Curation Spark Cluster_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0528-044328-qlwinjat"
		},
		"Synapse Serverless_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "trainingsynapse100-ondemand.sql.azuresynapse.net"
		},
		"Synapse Serverless_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "gold"
		},
		"Synapse Serverless_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladminuser"
		},
		"httpsource_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/PacktPublishing/Data-Engineering-with-Apache-Spark-Delta-Lake-and-Lakehouse/main/project/prep/iplocation/IP2LOCATION-LITE-DB1.CSV"
		},
		"restsource_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://prepaid.currconv.com/api/v7/convert?"
		},
		"salesdbsource_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "salesdb102.database.windows.net"
		},
		"salesdbsource_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "salesdb"
		},
		"salesdbsource_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "salesadmin"
		},
		"traininglakehouse_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://electraininglakehouse100.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "traininglakehousedf1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Curation Spark Cluster_accessToken": {
						"value": "[parameters('Curation Spark Cluster_accessToken')]"
					},
					"Synapse Serverless_password": {
						"value": "[parameters('Synapse Serverless_password')]"
					},
					"salesdbsource_password": {
						"value": "[parameters('salesdbsource_password')]"
					},
					"traininglakehouse_accountKey": {
						"value": "[parameters('traininglakehouse_accountKey')]"
					},
					"websitetrackinglogs_connectionString": {
						"value": "[parameters('websitetrackinglogs_connectionString')]"
					},
					"Curation Spark Cluster_properties_typeProperties_existingClusterId": {
						"value": "[parameters('Curation Spark Cluster_properties_typeProperties_existingClusterId')]"
					},
					"Synapse Serverless_properties_typeProperties_server": {
						"value": "[parameters('Synapse Serverless_properties_typeProperties_server')]"
					},
					"Synapse Serverless_properties_typeProperties_database": {
						"value": "[parameters('Synapse Serverless_properties_typeProperties_database')]"
					},
					"Synapse Serverless_properties_typeProperties_userName": {
						"value": "[parameters('Synapse Serverless_properties_typeProperties_userName')]"
					},
					"httpsource_properties_typeProperties_url": {
						"value": "[parameters('httpsource_properties_typeProperties_url')]"
					},
					"restsource_properties_typeProperties_url": {
						"value": "[parameters('restsource_properties_typeProperties_url')]"
					},
					"salesdbsource_properties_typeProperties_server": {
						"value": "[parameters('salesdbsource_properties_typeProperties_server')]"
					},
					"salesdbsource_properties_typeProperties_database": {
						"value": "[parameters('salesdbsource_properties_typeProperties_database')]"
					},
					"salesdbsource_properties_typeProperties_userName": {
						"value": "[parameters('salesdbsource_properties_typeProperties_userName')]"
					},
					"traininglakehouse_properties_typeProperties_url": {
						"value": "[parameters('traininglakehouse_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}